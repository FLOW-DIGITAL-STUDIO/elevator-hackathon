// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Player {
  id         String @id @default(cuid())
  firstname  String
  lastname   String
  goal       Int
  salary     Int
  devise     String
  pictureURl String @db.Text
}
