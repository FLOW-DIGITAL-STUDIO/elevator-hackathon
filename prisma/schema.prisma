generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["referentialIntegrity"]
}

datasource db {
    provider             = "mysql"
    url                  = env("DATABASE_URL")
    referentialIntegrity = "prisma"
}

model Joueurs {
    firstname String
    lastname  String
    goal     BigInt
    salary  BigInt
    id        Int      @id @default(autoincrement())
    devise String 
    pictureURl String
}