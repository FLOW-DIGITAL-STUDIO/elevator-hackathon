generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Player {
    id         Int    @id @default(autoincrement())
    firstname  String
    lastName   String
    goal       Int
    salary     Int
    device     String
    pictureURl String

    @@index([id])
}
