generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Player {
  id         Int    @id @default(autoincrement())
  firstName  String @db.VarChar(255)
  lastName   String @db.VarChar(255)
  goal       Int
  salary     Int
  devise     String @db.Char(10)
  pictureURL String @db.Char(255)
}
